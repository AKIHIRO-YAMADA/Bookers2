<%= render 'users/list',user:@user ,book:@book %>
<div class="container-fluid masonry-container">
 <div class="row">
  <div class="col-lg-9"> 
   <div class=”form-inline”>
   <h2>Users</h2>
    <table class="table table">
    <thead>
        <th>image</th>
         <th>name</th>
         <th colspan="2"></th>
    </thead>
   <tboby>
     <% @users.each do |user| %>
     <tr>
        <td>
          <%= attachment_image_tag user, :profile_image, :fill, 60, 60, format: 'jpeg', class: "img-square pull-left profile-img", fallback: "no_image.jpg" %> 
        </td>
        <td>
          <%= user.name %>
        </td>
        <td>
        <%= link_to "Show", user_path(user) %>
        </td> 
     </tr>
<% end %>
    </tboby>
   </table>
   <p align="right">CopyRight Infratop.inc</p>
   </div>
  </div>
 </div>
</div>
</form>
